{
  "name": "fabcar",
  "version": "1.0.0",
  "description": "FabCar application implemented in JavaScript",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node testGetUser.js && node testAddUserMul.js",
    "build:server": "npx parcel build ./server.js --target node -o run-server.js --out-dir ./",
    "build:web": "cd web && npm run build",
    "build": "npm run build:server && npm run build:web",
    "genIP": "node genIP.js",
    "start": "npm run genIP && npm run build && node ./run-server.js"
  },
  "engineStrict": true,
  "author": "Hyperledger",
  "license": "Apache-2.0",
  "dependencies": {
    "express": "^4.17.1",
    "fabric-ca-client": "beta",
    "fabric-network": "beta"
  },
  "devDependencies": {
    "axios": "^0.19.2",
    "chai": "^4.2.0",
    "cors": "^2.8.5",
    "eslint": "^5.9.0",
    "express": "^4.17.1",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "parcel-bundler": "^1.12.4",
    "rxjs": "^6.5.5",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.3.0"
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "test/**"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  }
}
